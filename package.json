{
  "name": "gql-schema-tools",
  "description": "Tools to work with graphql schemas",
  "version": "0.0.0",
  "author": "Ben Force",
  "bin": {
    "gql": "./bin/run"
  },
  "bugs": "https://github.com/drg-adaptive/gql-schema-tools/issues",
  "dependencies": {
    "@graphql-tools/load-files": "^6.2.4",
    "@graphql-tools/merge": "^6.2.4",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "graphql": "^15.3.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@tsconfig/node12": "^1.0.7",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/drg-adaptive/gql-schema-tools",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gql",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "drg-adaptive/gql-schema-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
